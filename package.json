{
    "name": "@proofrails/sdk",
    "version": "1.3.0",
    "description": "Beginner-friendly SDK for ProofRails ISO 20022 Middleware - Create blockchain-verifiable payment receipts with zero coding knowledge",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./react": {
            "types": "./dist/react.d.ts",
            "import": "./dist/react.mjs",
            "require": "./dist/react.js"
        }
    },
    "dependencies": {
        "@rainbow-me/rainbowkit": "^2.2.10",
        "@tanstack/react-query": "^5.90.12",
        "viem": "^2.43.3",
        "wagmi": "^2.19.5"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "files": [
        "dist",
        "README.md",
        "BEGINNER_GUIDE.md",
        "PRODUCTION_GUIDE.md",
        ".env.example"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "iso20022",
        "blockchain",
        "payments",
        "receipts",
        "flare",
        "compliance",
        "proofrails"
    ],
    "author": "ProofRails",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Elite-tch/Proofrails-sdk.git"
    },
    "homepage": "https://www.flarestudio.xyz/sdk/proofrails-sdk/overview",
    "bugs": {
        "url": "https://www.flarestudio.xyz/sdk/proofrails-sdk/issues",
        "email": "izuchukwujohnbosco95@gmail.com"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "@types/react": "^18.0.0",
        "wagmi": "^2.0.0",
        "viem": "^2.0.0"
    }
}